{
    "name": "pepperlabs/pepper",
    "description": "Pepper is a auto-generate GraphQL API for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Amirmasoud Sheydaei",
            "email": "amirmasoud.sheydaei@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "rebing/graphql-laravel": "^5.1",
        "haydenpierce/class-finder": "^0.4.2",
        "doctrine/dbal": "~2.6.0|~2.7.0|~2.8.0|~2.9.0|^2.10",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^7.5.15|^8.4|^9.0",
        "laravel/framework": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
        "orchestra/testbench": "~3.6.0|~3.7.0|~3.8.0|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Pepper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/orchestra/testbench-core/laravel/app",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pepper\\PepperServiceProvider"
            ],
            "aliases": {
                "Pepper": "Pepper\\Pepper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
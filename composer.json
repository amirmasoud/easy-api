{
  "name": "amirmasoud/pepper",
  "description": "Pepper is An easy API for Laravel",
  "homepage": "https://github.com/amirmasoud/pepper",
  "license": "MIT",
  "authors": [
    {
      "name": "Amirmasoud Sheydaei",
      "email": "amirmasoud.sheydaei@gmail.com",
      "homepage": "https://amirmasoud.me",
      "role": "Developer"
    }
  ],
  "require": {
    "doctrine/dbal": "^2.10",
    "rebing/graphql-laravel": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "Amirmasoud\\Pepper\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rebing\\GraphQL\\GraphQLServiceProvider",
        "Amirmasoud\\Pepper\\PepperServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
